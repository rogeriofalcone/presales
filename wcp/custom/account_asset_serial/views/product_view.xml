<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>


        <record id="product_template_form_asset_view" model="ir.ui.view">
            <field name="name">product.template.product.form.assset.view</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_form_view"/>
            <field name="arch" type="xml">
       			<xpath expr="//field[@name='property_account_income']" position="before">
                    <label for="asset_categ" string="Asset Category"/>
                    <div><field name="asset_categ" colspan="3" nolabel="1"/></div>
       			</xpath>     	
            </field>
       </record>



</data>
</openerp>
